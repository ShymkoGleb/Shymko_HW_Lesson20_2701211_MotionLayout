<?xml version="1.0" encoding="utf-8"?>
<MotionScene xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto">
    <Transition
        app:constraintSetEnd="@layout/activity_main_end"
        app:constraintSetStart="@layout/activity_main_start"
        app:duration="3000">
        <OnSwipe
            app:dragDirection="dragUp"
            app:touchAnchorId="@id/ivLogo8"
            app:touchAnchorSide="bottom" />
        <KeyFrameSet>
            <KeyPosition
                app:framePosition="30"
                app:keyPositionType="deltaRelative"
                app:motionTarget="@id/ivLogo4"
                app:percentX="1"
                app:percentY="0" />
            <KeyPosition
                app:framePosition="30"
                app:keyPositionType="deltaRelative"
                app:motionTarget="@id/ivLogo5"
                app:percentX="1"
                app:percentY="0" />
            <KeyPosition
                app:framePosition="30"
                app:keyPositionType="deltaRelative"
                app:motionTarget="@id/ivLogo6"
                app:percentX="1"
                app:percentY="0" />
            <KeyAttribute
                android:alpha="0"
                app:framePosition="30"
                app:motionTarget="@id/ivLogo7"/>
            <KeyAttribute
                android:alpha="0"
                app:framePosition="30"
                app:motionTarget="@id/ivLogo8"/>
            <KeyAttribute
                android:alpha="0"
                app:framePosition="30"
                app:motionTarget="@id/ivLogo9"/>
            <KeyPosition
                app:framePosition="80"
                app:keyPositionType="deltaRelative"
                app:motionTarget="@id/ivLogo7"
                app:percentY="1"
                app:percentX="0"/>
            <KeyPosition
                app:framePosition="80"
                app:keyPositionType="deltaRelative"
                app:motionTarget="@id/ivLogo8"
                app:percentY="1"
                app:percentX="0"/>
            <KeyPosition
                app:framePosition="80"
                app:keyPositionType="deltaRelative"
                app:motionTarget="@id/ivLogo9"
                app:percentY="1"
                app:percentX="0"/>
        </KeyFrameSet>
    </Transition>
</MotionScene>